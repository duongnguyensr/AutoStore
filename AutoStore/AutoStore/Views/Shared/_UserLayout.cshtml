    <script>
        $(function () {
            //        initialize modals
            $('#LoginModal').modal({ ending_top: '50%' });

            //        draw two lines on canvas
            let jcanvas = $('canvas');
            let canvas = jcanvas[0];
            let width = canvas.width = jcanvas.width();
            let height = canvas.height = jcanvas.height();
            let context = canvas.getContext("2d");

            function drawLine(y0, x0, y1, x1) {
                context.beginPath();
                context.moveTo(x0, y0);
                context.lineTo(x1, y1);
                context.stroke();
            }

            let h = height * .4;
            let w = width * .55;
            context.fillRect((width - w) / 2, (height - h) / 2, w, h);
            drawLine(height / 2, 0, height / 2, width);
            drawLine(0, width / 2, height, width / 2);
        });
    </script>
